    "configurations": [
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processName": "godot"
        },

        {
            "name": "Launch GodotDemoProject",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "./tools/godot/godot.exe",
            "args": [],
            "cwd": "${workspaceFolder}/GodotDemoProject",
            "stopAtEntry": false,
        }


    ]
}